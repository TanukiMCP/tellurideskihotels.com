---
export const prerender = false;

const url = Astro.url;
const searchParams = url.searchParams;

// Redirect all /lodging routes to /places-to-stay
const pathname = url.pathname;
const queryString = searchParams.toString();

return Astro.redirect(pathname.replace('/lodging', '/places-to-stay') + (queryString ? `?${queryString}` : ''), 301);
---
